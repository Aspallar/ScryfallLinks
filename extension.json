{
	"name": "ScryfallLinks",
	"version": "0.1.0",
	"author": [
		"Nils Enevoldsen"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ScryfallLinks",
	"descriptionmsg": "scryfalllinks-desc",
	"license-name": "MIT",
	"type": "parserhook",
	"AutoloadClasses": {
		"MediaWiki\\Extension\\ScryfallLinks\\Hooks": "src/Hooks.php",
		"MediaWiki\\Extension\\ScryfallLinks\\SpecialHelloWorld": "src/SpecialHelloWorld.php"
	},
	"ConfigRegistry": {
		"scryfalllinks": "GlobalVarConfig::newInstance"
	},
	"config": {
		"ScryfallLinksEnableFoo": {
			"value": true,
			"description": "Whether or not the foo feature is enabled."
		}
	},
	"ExtensionMessagesFiles": {
		"ScryfallLinksAlias": "ScryfallLinks.i18n.alias.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"MediaWiki\\Extension\\ScryfallLinks\\Hooks::onParserFirstCallInit"
		]
	},
	"MessagesDirs": {
		"ScryfallLinks": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.scryfallLinks.foo": {
			"scripts": [
				"ext.scryfallLinks.js",
				"ext.scryfallLinks.foo.js",
				"ext.scryfallLinks.tooltip.js"
			],
			"styles": [
				"ext.scryfallLinks.foo.css"
			],
			"messages": [],
			"dependencies": [
				"oojs-ui-core",
				"oojs-ui-widgets"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "ScryfallLinks/modules"
	},
	"SpecialPages": {
		"HelloWorld": "MediaWiki\\Extension\\ScryfallLinks\\SpecialHelloWorld"
	},
	"manifest_version": 2
}
